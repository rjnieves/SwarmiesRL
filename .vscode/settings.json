{
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-output-directory=%DIR%/tex-output",
        "%DOC%"
      ]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-output-directory=%DIR%/tex-output",
        "%DOC%"
      ]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": [
          "%DOCFILE%"
      ]
    }
  ],
  "latex-workshop.latex.outDir": "%DIR%/tex-output",
  "latex-workshop.latex.autoBuild.run": "onFileChange",
  "latex-workshop.latex.watch.files.ignore": [
    "**/*.bbx",
    "**/*.bbl",
    "**/*.cbx",
    "**/*.cfg",
    "**/*.clo",
    "**/*.cnf",
    "**/*.def",
    "**/*.fmt",
    "**/*.lbx",
    "**/*.map",
    "**/*.pfb",
    "**/*.tfm",
    "**/texmf-{dist,var}/**",
    "/dev/null"
  ],
  "cSpell.words": [
    "ANNs",
    "Kalman",
    "Robo",
    "SARSA",
    "Swarmathon",
    "Swarmie",
    "Swarmie's",
    "deconflicting",
    "discretization",
    "discretized",
    "fetchable",
    "hyperparameters",
    "odometry",
    "stateful",
    "subcomponent",
    "swarmies",
    "waypoint",
    "waypoints"
  ],
  "cmake.configureOnOpen": false,
  "clangd.arguments": [
    "-compile-commands-dir=${workspaceRoot}/build"
  ],
  "cmake.buildDirectory": "${workspaceRoot}/build",
  "files.watcherExclude": {
    "**/build": true,
    "**/.catkin_tools": true,
    "**/logs": true
  },
  "editor.wordWrap": "wordWrapColumn",
  "python.pythonPath": "~/pythonenv/SWARMIES-RL/bin/python"
}
